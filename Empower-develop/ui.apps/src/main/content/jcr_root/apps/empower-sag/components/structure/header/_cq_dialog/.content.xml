<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <loginSectionEnabler
                        jcr:primaryType="nt:unstructured"
                        sling:orderBefore="navigationenabler"
                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                        name="./loginSectionEnabler"
                        text="Enable Login Section"
                        uncheckedValue="{Boolean}false"
                        value="{Boolean}true"/>
                    <logoutLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="The text to display on the Logout button."
                        fieldLabel="Logout Button Label"
                        name="./logoutLabel"/>
                    <file
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                        allowUpload="{Boolean}false"
                        autoStart="{Boolean}false"
                        class="cq-droptarget"
                        fieldDescription="Please add icon here."
                        fieldLabel="Logout Icon"
                        fileNameParameter="./fileName"
                        fileReferenceParameter="./fileReference"
                        mimeTypes="[image/.*]"
                        multiple="{Boolean}false"
                        name="./filea"
                        title="Asset"
                        useHTML5="{Boolean}true"/>
                    <alttext
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Please enter Alt text here"
                        fieldLabel="Alternative Text"
                        name="./altTexta"
                        required="{Boolean}false"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
