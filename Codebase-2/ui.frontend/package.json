{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-project-archetype"
  },
  "private": true,
  "main": "src/main/webpack/site/core.js",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "eslint:fix": "eslint --fix './src/main/webpack/**/*.js'",
    "build:dev": "cross-env webpack -d --env dev --config ./webpack.dev.js",
    "build:prod": "cross-env webpack -p --config ./webpack.prod.js",
    "start": "cross-env FORCE_COLOR=1 CI=true concurrently --prefix \"[ {name} ]\"  --names \"AEMFed, Webpack \" --prefix-colors \"white.bgCyan.bold,white.bgGreen.bold\" \"npm run aem:watch\" \"npm run watch:dev\" --kill-others",
    "watch:dev": "cross-env webpack -d --config ./webpack.dev.js --watch",
    "aem:watch": "cross-env aemfed -t http://admin:admin@localhost:4502 -w ../ui.apps/src/main/content/jcr_root",
    "build:clientlibs": "clientlib --verbose",
    "stylelint": "stylelint '**/*.scss' --fix --config ./.stylelintrc.json; exit 0",
    "postinstall": "cross-env npm rebuild node-sass",
    "visual-test": "backstop test --docker",
    "visual-approve": "backstop approve --docker"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-do-expressions": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-stage-1": "^7.8.3",
    "@babel/preset-typescript": "^7.12.7",
    "acorn": "^6.4.1",
    "aem-clientlib-generator": "^1.7.5",
    "aemfed": "^0.1.1",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "backstopjs": "^5.0.7",
    "browserslist": "^4.15.0",
