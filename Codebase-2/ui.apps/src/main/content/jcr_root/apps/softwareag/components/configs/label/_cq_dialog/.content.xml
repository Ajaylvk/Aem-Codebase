<?xml version="1.0" encoding="UTF-8"?>
<jcr:root
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Label Configuration"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}false">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <labelLearnMore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Learn more&quot;"
                                        required="{Boolean}true"
                                        name="./labelLearnMore" />
                                    <labelLoadMore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Load more&quot;"
                                        required="{Boolean}true"
                                        name="./labelLoadMore" />
                                    <labelGoToOverview
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Go to overview&quot;"
                                        required="{Boolean}true"
                                        name="./labelGoToOverview" />
                                    <labelBack
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Back&quot;"
                                        required="{Boolean}true"
                                        name="./labelBack" />
                                    <labelOverview
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Overview&quot;"
                                        required="{Boolean}true"
                                        name="./labelOverview" />
                                </items>
                            </column>
                        </items>
                    </general>
                    <event
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Event"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <labelDays
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Days&quot;"
                                        required="{Boolean}true"
                                        name="./labelDays" />
                                    <labelHours
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Hours&quot;"
                                        required="{Boolean}true"
                                        name="./labelHours" />
                                    <labelMinutes
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Minutes&quot;"
                                        required="{Boolean}true"
                                        name="./labelMinutes" />
                                    <labelSeconds
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Seconds&quot;"
                                        required="{Boolean}true"
                                        name="./labelSeconds" />
                                    <labelEventFilterOptionOne
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Event Filter Option 1&quot;"
                                        required="{Boolean}true"
                                        name="./labelEventFilterOptionOne" />
                                    <labelEventFilterOptionTwo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Event Filter Option 2&quot;"
                                        required="{Boolean}true"
                                        name="./labelEventFilterOptionTwo" />
                                </items>
                            </column>
                        </items>
                    </event>
                    <partner
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Partner"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <labelCountries
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Countries&quot;"
                                        required="{Boolean}true"
                                        name="./labelCountries" />
                                    <labelPartnerType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Partner type&quot;"
                                        required="{Boolean}true"
                                        name="./labelPartnerType" />
                                    <labelProductCategory
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Product category&quot;"
                                        required="{Boolean}true"
                                        name="./labelProductCategory" />
                                    <labelIndustries
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Industries&quot;"
                                        required="{Boolean}true"
                                        name="./labelIndustries" />
                                    <labelCrossIndustry
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Cross Industry&quot;"
                                        required="{Boolean}true"
                                        name="./labelCrossIndustry" />
                                    <labelIndustry
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Industry&quot;"
                                        required="{Boolean}true"
                                        name="./labelIndustry" />
                                </items>
                            </column>
                        </items>
                    </partner>
                    <assets
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Assets"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <labelWatchVideo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Watch Video&quot;"
                                        required="{Boolean}true"
                                        name="./labelWatchVideo" />
                                    <labelReadArticle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Label &quot;Read Article&quot;"
                                        required="{Boolean}true"
                                        name="./labelReadArticle" />
                                </items>
                            </column>
                        </items>
                    </assets>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
