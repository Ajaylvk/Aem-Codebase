<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Nav Card"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <variation
                        granite:class="cq-dialog-dropdown-showhide"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                        cq-dialog-dropdown-showhide-target=".comp-showhide-target"
                        fieldDescription="Variation of the component"
                        fieldLabel="Component Variation"
                        name="./compvariation">
                        <items jcr:primaryType="nt:unstructured">
                            <variation_1
                                jcr:primaryType="nt:unstructured"
                                text="Nav Card A"
                                value="variationa"/>
                            <variation_2
                                jcr:primaryType="nt:unstructured"
                                text="Nav Card B"
                                value="variationb"/>
                            <variation_3
                                jcr:primaryType="nt:unstructured"
                                text="Nav Card C"
                                value="variationc"/>
                        </items>
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            cq-dialog-dropdown-showhide-target=".comp-showhide-target"/>
                    </variation>
                    <variation_1
                        granite:class="comp-showhide-target"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            showhidetargetvalue="variationa"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <file
                                        granite:id="dmFileReference"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldDescription="Please add icon here."
                                        fieldLabel="Icon"
                                        fileNameParameter="./fileNamea"
                                        fileReferenceParameter="./fileReferencea"
                                        mimeTypes="[image/.*]"
                                        multiple="{Boolean}false"
                                        name="./filea"
                                        title="Asset"
                                        useHTML5="{Boolean}true"/>
                                    <alttext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter Alt text here"
                                        fieldLabel="Alternative Text"
                                        name="./altTexta"
                                        required="{Boolean}false"/>
                                    <iconbgcolor
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="The Background color of the Icon"
                                        fieldLabel="Icon Color"
                                        name="./iconBackgroundColora">
                                        <items jcr:primaryType="nt:unstructured">
                                            <select
                                                jcr:primaryType="nt:unstructured"
                                                text="Select Icon Color"
                                                value=""/>
                                            <white
                                                jcr:primaryType="nt:unstructured"
                                                text="White"
                                                value="input__colorselect-icon-color8"/>
                                            <coredark120
                                                jcr:primaryType="nt:unstructured"
                                                text="Core Dark 120"
                                                value="input__colorselect-icon-color8"/>
                                            <brightpulse600
                                                jcr:primaryType="nt:unstructured"
                                                text="Bright Pulse 600"
                                                value="input__colorselect-icon-color2"/>
                                            <brightsense300
                                                jcr:primaryType="nt:unstructured"
                                                text="Bright Sense 300"
                                                value="input__colorselect-icon-color11"/>
                                            <brightsense500
                                                jcr:primaryType="nt:unstructured"
                                                text="Bright Sense 500 "
                                                value="input__colorselect-icon-color3"/>
                                        </items>
                                    </iconbgcolor>
                                </items>
                            </column>
                        </items>
                    </variation_1>
                    <variation_2
                        granite:class="comp-showhide-target"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            showhidetargetvalue="variationb"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <file
                                        granite:id="dmFileReference"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldDescription="Please add asset here."
                                        fieldLabel="Icon"
                                        fileNameParameter="./fileNameb"
                                        fileReferenceParameter="./fileReferenceb"
                                        mimeTypes="[image/.*]"
                                        multiple="{Boolean}false"
                                        name="./fileb"
                                        title="Asset"
                                        useHTML5="{Boolean}true"/>
