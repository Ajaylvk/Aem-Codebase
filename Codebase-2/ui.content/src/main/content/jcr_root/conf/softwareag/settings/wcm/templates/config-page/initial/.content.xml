<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/softwareag/settings/wcm/templates/config-page"
        jcr:primaryType="cq:PageContent"
        sling:redirect="{Boolean}true"
        sling:redirectStatus="{Long}302"
        sling:resourceType="softwareag/components/structure/configurationpage"
        hideInNav="{Boolean}true">
        <general-config
            jcr:primaryType="nt:unstructured"
            sling:resourceType="softwareag/components/configs/general-config"
            editable="{Boolean}true">
        </general-config>
        <parsys
            jcr:primaryType="nt:unstructured"
            sling:resourceType="softwareag/components/content/parsys" />
    </jcr:content>
</jcr:root>
