<?xml version="1.0" encoding="UTF-8"?>
<jcr:root
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/softwareag/settings/wcm/templates/map"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="softwareag/components/structure/basic-page">
        <teaserbanner
            jcr:primaryType="nt:unstructured"
            editable="{Boolean}true"
            sling:resourceType="softwareag/components/content/teaserbanner" />
        <title
            jcr:primaryType="nt:unstructured"
            editable="{Boolean}true"
            sling:resourceType="softwareag/components/content/title" />
    </jcr:content>
</jcr:root>

