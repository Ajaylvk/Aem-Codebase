<jcr:root
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Offices"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <america
                        jcr:primaryType="nt:unstructured"
                        jcr:title="America"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <items
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                jcr:primaryType="nt:unstructured"
                                                composite="{Boolean}true">
                                                <field sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./americaOffice">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <items
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                                            path="/apps/softwareag/components/content/map/tab_continent" />
                                                    </items>
                                                </field>
                                            </items>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </america>
                    <europe
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Europe"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <items
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                jcr:primaryType="nt:unstructured"
                                                composite="{Boolean}true">
                                                <field sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./europeOffice">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <items
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                                            path="/apps/softwareag/components/content/map/tab_continent" />
                                                    </items>
                                                </field>
                                            </items>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </europe>
                    <africa
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Africa"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <items
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                jcr:primaryType="nt:unstructured"
                                                composite="{Boolean}true">
                                                <field sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./africaOffice">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <items
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                                            path="/apps/softwareag/components/content/map/tab_continent" />
                                                    </items>
                                                </field>
                                            </items>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </africa>
                    <asia
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Asia"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <items
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                jcr:primaryType="nt:unstructured"
                                                composite="{Boolean}true">
                                                <field sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./asiaOffice">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <items
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                                            path="/apps/softwareag/components/content/map/tab_continent" />
                                                    </items>
                                                </field>
                                            </items>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </asia>
                    <australia
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Australia"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <items
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                jcr:primaryType="nt:unstructured"
                                                composite="{Boolean}true">
                                                <field sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./australiaOffice">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <items
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/include"
                                                            path="/apps/softwareag/components/content/map/tab_continent" />
                                                    </items>
                                                </field>
                                            </items>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </australia>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
